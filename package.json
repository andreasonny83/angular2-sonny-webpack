{
  "name": "angular2-sonny-webpack",
  "version": "1.1.2",
  "description": "A Webpack starter kit for you Angular2 progressive web app",
  "keywords": [
    "angular2",
    "webpack",
    "typescript"
  ],
  "author": "Andrea Sonny <andreasonny83@gmail.com>",
  "homepage": "https://github.com/andreasonny83/angular2-sonny-webpack#readme",
  "license": "MIT",
  "scripts": {
    "build:dev": "webpack --config config/webpack.dev.js --progress --profile",
    "build:prod": "webpack --config config/webpack.prod.js  --progress --profile --bail",
    "build": "npm run build:prod",
    "ci": "npm run lint && npm test && npm run e2e",
    "clean:dist": "npm run rimraf -- dist",
    "clean:install": "npm set progress=false && npm install",
    "clean:start": "npm start",
    "clean": "npm cache clean && npm run rimraf -- node_modules doc coverage dist",
    "docs": "npm run typedoc -- --options typedoc.json --exclude '**/*.spec.ts' ./src/",
    "e2e:live": "npm run e2e -- --elementExplorer",
    "e2e": "npm run protractor",
    "lint": "npm run tslint \"src/**/*.ts\"",
    "prebuild:dev": "npm run clean:dist",
    "prebuild:prod": "npm run clean:dist",
    "postbuild:prod": "npm run precache",
    "preclean:install": "npm run clean",
    "preclean:start": "npm run clean",
    "postinstall": "npm run webdriver:update",
    "protractor": "protractor",
    "rimraf": "rimraf",
    "server:dev": "webpack-dev-server --config config/webpack.dev.js --progress --profile --watch --content-base src/",
    "server:prod": "lite-server --config=config/bs-config.json",
    "server": "npm run server:dev",
    "start": "npm run server:dev",
    "test": "karma start",
    "tslint": "tslint",
    "typedoc": "typedoc",
    "watch:dev": "npm run build:dev -- --watch",
    "watch:prod": "npm run build:prod -- --watch",
    "watch:test": "npm run test -- --auto-watch --no-single-run",
    "watch": "npm run watch:dev",
    "webdriver-manager": "webdriver-manager",
    "webdriver:start": "npm run webdriver-manager start",
    "webdriver:update": "npm run webdriver-manager update",
    "webpack-dev-server": "webpack-dev-server",
    "webpack": "webpack",
    "precache": "sw-precache --root=dist --config=config/sw-precache-config.js --verbose"
  },
  "dependencies": {
    "@angular/common": "2.4.1",
    "@angular/compiler": "2.4.1",
    "@angular/core": "2.4.1",
    "@angular/forms": "2.4.1",
    "@angular/http": "2.4.1",
    "@angular/platform-browser": "2.4.1",
    "@angular/platform-browser-dynamic": "2.4.1",
    "@angular/router": "3.4.1",
    "assets-webpack-plugin": "3.5.0",
    "core-js": "2.4.1",
    "rxjs": "5.0.2",
    "zone.js": "0.7.4"
  },
  "devDependencies": {
    "@types/jasmine": "2.5.40",
    "@types/node": "6.0.55",
    "@types/selenium-webdriver": "2.53.38",
    "@types/source-map": "0.5.0",
    "@types/uglify-js": "2.6.28",
    "@types/webpack": "2.1.0",
    "angular-router-loader": "0.4.0",
    "angular2-template-loader": "0.6.0",
    "awesome-typescript-loader": "2.2.4",
    "codelyzer": "2.0.0-beta.2",
    "copy-webpack-plugin": "4.0.0",
    "css-loader": "0.26.1",
    "file-loader": "^0.9.0",
    "html-webpack-plugin": "2.21.0",
    "istanbul-instrumenter-loader": "1.2.0",
    "jasmine-core": "2.5.2",
    "karma": "1.3.0",
    "karma-chrome-launcher": "2.0.0",
    "karma-coverage": "1.1.1",
    "karma-jasmine": "1.1.0",
    "karma-mocha-reporter": "2.2.1",
    "karma-remap-coverage": "0.1.4",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.1",
    "lite-server": "^2.2.2",
    "protractor": "4.0.14",
    "rimraf": "2.5.4",
    "script-ext-html-webpack-plugin": "1.3.2",
    "source-map-loader": "0.1.5",
    "sw-precache": "^4.2.3",
    "to-string-loader": "1.1.5",
    "ts-helpers": "1.1.2",
    "ts-node": "2.0.0",
    "tslint": "~4.0.0",
    "typedoc": "0.5.3",
    "typescript": "2.1.1",
    "webpack": "2.1.0-beta.27",
    "webpack-dev-middleware": "1.6.1",
    "webpack-dev-server": "2.1.0-beta.11",
    "webpack-md5-hash": "0.0.5",
    "webpack-merge": "1.0.1"
  },
  "repository": "git@github.com:andreasonny83/angular2-sonny-webpack.git",
  "bugs": {
    "url": "https://github.com/andreasonny83/angular2-sonny-webpack/issues"
  },
  "private": true,
  "main": "index.js"
}
